<form class="signup-form">
  <div>
    <label for="first-name-signup">First name:</label>
    <input type="text" id="first-name-signup" />
  </div>
  <div>
    <label for="last-name-signup">Last name:</label>
    <input type="text" id="last-name-signup" />
  </div>
  <div>
    <label for="email-signup">email:</label>
    <input type="text" id="email-signup" />
  </div>
  <div>
    <label for="phone-signup">Phone number:</label>
    <input type="text" id="phone-signup" />
  </div>
  <div>
    <label for="zipcode-signup">Zipcode:</label>
    <input type="text" id="zipcode-signup" />
  </div>
  <div>
    <label for="password-signup">password:</label>
    <input type="password" id="password-signup" />
  </div>
  <form>
        <div>
            <input id="picture" type="hidden" role="uploadcare-uploader" name="my_picture" />
        </div> 
    </form>
  <div>
    <button type="submit">Signup</button>
  </div>
</form>
<script src="https://code.jquery.com/jquery-3.4.1.min.js" charset="utf-8"></script>
