<section class="motto justify-content-evenly">
  <div>
<form class="signup-form">
  <div class="form-group">
    <label for="first-name-signup">First name</label>
    <input type="text" class="form-control" id="first-name-signup" placeholder="First name">
  </div>
  <div class="form-group">
    <label for="last-name-signup">Phone</label>
    <input type="password" class="form-control" id="last-name-signup" placeholder="Last name">
  </div>
  <div class="form-group">
    <label for="email-signup">Email address</label>
    <input type="email" class="form-control" id="email-signup" aria-describedby="emailHelp" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="phone-signup">Phone</label>
    <input type="text" class="form-control" id="phone-signup" placeholder="Phone">
  </div>
   <div class="form-group">
    <label for="zipcode-signup">Zipcode</label>
    <input type="text" class="form-control" id="zipcode-signup" placeholder="Zipcode">
  </div>
  <div class="form-group">
    <label for="password-signup">Password</label>
    <input type="password" class="form-control" id="password-signup" placeholder="Password">
  </div>
  <div class="form-group">
    <label for="password-confirm">Confirm password</label>
    <input type="password" class="form-control" id="password-comfirm" placeholder="Confirm password">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>
</section>
<script src="/javascript/signup.js"></script>
