<div class="container">
    <section>
        <form class="sitterSignup-form mt-3">
            <h4>PET SITTER SIGNUP</h4>
            <div class="form-group">
                <label for="hosting-signup">Hosting Preference</label>
                <input type="text" class="form-control" id="hosting-signup" placeholder="Enter Hosting Preference">
            </div>
            <div class="form-group">
                <label for="bio-signup">Bio</label>
                <input type="text" class="form-control" id="bio-signup" placeholder="Enter Bio">
            </div>
            <div class="form-group">
                <label for="socials-signup">Social Media Account</label>
                <input type="text" class="form-control" id="socials-signup" placeholder="Enter Social Media Account">
            </div>
            <div class="form-group">
                <label for="contact-signup">Contact Preference</label>
                <input type="text" class="form-control" id="contact-signup" placeholder="Enter Contact Preference">
            </div>
            <div class="form-group">
                <label for="petSkills-signup">Pet Skills</label>
                <input type="text" class="form-control" id="petSkills-signup" placeholder="Enter Pet Skills">
            </div>
            <div class="form-group">
                <label for="price-signup">Price Per Day</label>
                <input type="text" class="form-control" id="price-signup" placeholder="Enter Price Per Day">
            </div>
            <div class="form-group">
                <label for="meutered-signup">Neutered Pets Only?</label>
                <input type="checkbox" class="" id="meutered-signup" value="true">
            </div>
            <div class="form-group">
                <label for="availability-signup">Avaiable Times</label>
                <input type="text" class="form-control" id="availability-signup" placeholder="Enter Avaiable Times">
            </div>
            <button type="submit" class="btn btn-info btn-lg btn-block btn-signup">Submit</button>
        </form>
    </section>
</div>

<script src="/javascript/sitterSignup.js"></script>